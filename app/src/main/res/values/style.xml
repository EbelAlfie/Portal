<?xml version="1.0" encoding="utf-8"?>
<resources>
  <style name="textview_general_white">
    <item name="android:textColor">@color/white</item>
    <item name="android:fontFamily">@font/somatic</item>
  </style>

  <style name="textview_big" parent="textview_general_white">
    <item name="android:textSize">@dimen/text_30</item>
  </style>

  <style name="textview_header" parent="textview_big">
    <item name="android:textStyle">bold</item>
  </style>

  <style name="textview_medium" parent="textview_general_white">
    <item name="android:textSize">@dimen/text_20</item>
  </style>

  <style name="diamond_image">
    <item name="cornerFamily">rounded</item>
    <item name="cornerSize">80%</item>
  </style>

</resources>